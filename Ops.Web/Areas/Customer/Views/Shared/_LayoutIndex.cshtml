<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="description" content="">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- The above 4 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <!-- Title -->
    <title>@ViewData["Title"]</title>
    @*"~/productdetailcss/detailscustom.css"*@
    <link rel="stylesheet" href="~/productDetailCss/DetailsCustom.css" />
 
    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;500&family=Playfair+Display:wght@600;700&display=swap" rel="stylesheet">

    <!-- Icon Font Stylesheet -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">

    <!-- Libraries Stylesheet -->
    <link href="~/baker/lib/animate/animate.min.css" rel="stylesheet">

    <link href="~/baker/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">

    <!-- Customized Bootstrap Stylesheet -->
    <link href="~/baker/css/bootstrap.min.css" rel="stylesheet">

    <!-- Template Stylesheet -->
    <link href="~/baker/css/style.css" rel="stylesheet">





    <!-- Stylesheet -->
    <link rel="stylesheet" href="~/one-music-gh-pages/style.css">
    <link href="~/css/core-style.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Ops.Web.styles.css" asp-append-version="true" />
    <link href="~/productdetailcss/core-style.css" rel="stylesheet" />
    <link href="~/footer/main.css" rel="stylesheet" />
    <link href="~/footer/util.css" rel="stylesheet" />

</head>
<body>
     <!-- Spinner Start -->
    <div id="spinner" class="show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center">
        <div class="spinner-grow text-primary" role="status"></div>
    </div>
    <!-- Spinner End -->

    @Html.Partial("_Header") 

 
    @RenderBody()


    @Html.Partial("_Footer")

    <!-- ##### All Javascript Script ##### -->

    <script src="~/footer/main.js"></script>
    <script src="~/productdetailcss/active.js"></script>
    <script src="~/productdetailcss/plugins.js"></script>
    <script src="~/productdetailcss/popper.min.js"></script>
    <script src="~/footer/jquery-3.2.1.min.js"></script>
    <!-- jQuery-2.2.4 js -->
    <script src="~/one-music-gh-pages/js/jquery/jquery-2.2.4.min.js"></script>
    <!-- Popper js -->
    <script src="~/one-music-gh-pages/js/bootstrap/popper.min.js"></script>
    <!-- Bootstrap js -->
    <script src="~/one-music-gh-pages/js/bootstrap/bootstrap.min.js"></script>
    <!-- Active js -->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/one-music-gh-pages/js/plugins/plugins.js"></script>
    <script src="~/one-music-gh-pages/js/active.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>


    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/baker/lib/wow/wow.min.js"></script> 
     <script src="~/baker/lib/easing/easing.min.js"></script> 
    <script src="~/baker/lib/waypoints/waypoints.min.js"></script> 
     <script src="~/baker/lib/counterup/counterup.min.js"></script> 
     <script src="~/baker/lib/owlcarousel/owl.carousel.min.js"></script>


    <script src="~/baker/js/main.js"></script>



    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
