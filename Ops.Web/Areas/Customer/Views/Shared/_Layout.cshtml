    <!-- Libraries Stylesheet -->
    <link href="~/baker/lib/animate/animate.min.css" rel="stylesheet">

    <link href="~/baker/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">

    <!-- Favicon -->
    <link rel="icon" href="~/one-music-gh-pages/img/core-img/favicon.ico">
    <link rel="icon" href="~/amado-master/img/core-img/favicon.ico">
    <!-- Core Style CSS -->
    <link rel="stylesheet" href="~/amado-master/css/core-style.css">
