<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="description" content="">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- The above 4 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <!-- Title -->
    <title>@ViewData["Title"]</title>
    @*"~/productdetailcss/detailscustom.css"*@
    <link rel="stylesheet" href="~/productDetailCss/DetailsCustom.css"  />
    <!-- Favicon -->
    <link rel="icon" href="~/one-music-gh-pages/img/core-img/favicon.ico">

    <!-- Stylesheet -->
    <link rel="stylesheet" href="~/one-music-gh-pages/style.css">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Ops.Web.styles.css" asp-append-version="true" />
    <link href="~/productdetailcss/core-style.css" rel="stylesheet" />
    <link href="~/footer/main.css" rel="stylesheet" />
    <link href="~/footer/util.css" rel="stylesheet" /> 

</head>
<body>
   <!-- Preloader -->
    <div class="preloader d-flex align-items-center justify-content-center">
        <div class="lds-ellipsis">
            <div></div>
            <div></div>
            <div></div>
            <div></div>
        </div>
    </div> 
 
    @Html.Partial("_Header")

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    @Html.Partial("_Footer")

    <!-- ##### All Javascript Script ##### -->

     <script src="~/footer/main.js"></script>
    <script src="~/productdetailcss/active.js"></script>
    <script src="~/productdetailcss/plugins.js"></script>
    <script src="~/productdetailcss/popper.min.js"></script>
    <script src="~/footer/jquery-3.2.1.min.js"></script>
    <!-- jQuery-2.2.4 js -->
    <script src="~/one-music-gh-pages/js/jquery/jquery-2.2.4.min.js"></script>
    <!-- Popper js -->
    <script src="~/one-music-gh-pages/js/bootstrap/popper.min.js"></script>
    <!-- Bootstrap js -->
    <script src="~/one-music-gh-pages/js/bootstrap/bootstrap.min.js"></script>
    <!-- Active js -->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/one-music-gh-pages/js/plugins/plugins.js"></script>
    <script src="~/one-music-gh-pages/js/active.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
  
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
