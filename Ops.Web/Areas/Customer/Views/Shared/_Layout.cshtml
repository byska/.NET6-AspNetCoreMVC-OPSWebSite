    <link href="~/footer/main.css" rel="stylesheet" />
    <link href="~/footer/util.css" rel="stylesheet" />

</head>
<body>
     <!-- Spinner Start -->
    <div id="spinner" class="show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center">
        <div class="spinner-grow text-primary" role="status"></div>
    </div>
    <!-- Spinner End -->
    <!-- Navbar Start -->
    <nav class="navbar navbar-expand-lg navbar-dark fixed-top py-lg-0 px-lg-5 wow fadeIn" data-wow-delay="0.1s">
        <a asp-action="Index" asp-controller="Home" class="navbar-brand ms-4 ms-lg-0">
            <h1 class="text-primary m-0">OPS</h1>
        </a>
        <button type="button" class="navbar-toggler me-4" data-bs-toggle="collapse" data-bs-target="#navbarCollapse">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarCollapse">
            <div class="navbar-nav mx-auto p-4 p-lg-0">
                <a asp-action="Index" asp-controller="Home" class="nav-item nav-link active">Anasayfa</a>
                <a asp-action="About" asp-controller="Home" class="nav-item nav-link">Hakkımızda</a>
                <a asp-action="GetAllProduct" asp-controller="Product" asp-route-id="1" class="nav-item nav-link">Tişört Regular</a>
                <a asp-action="GetAllProduct" asp-controller="Product" asp-route-id="2" class="nav-item nav-link">Tişört Oversize</a>
                <a asp-action="GetAllProduct" asp-controller="Product" asp-route-id="3" class="nav-item nav-link">Hoodie</a>
                <a asp-action="GetAllProduct" asp-controller="Product" asp-route-id="4" class="nav-item nav-link">Sweatshirt</a>
                <div class="nav-item dropdown">
                    <a href="#" class="nav-link dropdown-toggle" data-bs-toggle="dropdown">Aksesuar</a>
                    <div class="dropdown-menu m-0">
                        <a asp-action="GetAllProduct" asp-controller="Product" asp-route-id="5" class="dropdown-item">Kol Çantası</a>
                        <a asp-action="GetAllProduct" asp-controller="Product" asp-route-id="6" class="dropdown-item">Cüzdan</a>
                        <a asp-action="GetAllProduct" asp-controller="Product" asp-route-id="7" class="dropdown-item">Baf</a>
                    </div>
                </div>
                <a asp-action="Contact" asp-controller="Home" class="nav-item nav-link">İletişim</a>
            </div>
            <div class=" d-none d-lg-flex">
                <div class="login-register-cart-button d-flex align-items-center">
                    <!-- Login/Register -->
                    <div class="login-register-btn mr-50">
                        <a href="login.html" id="loginBtn">Giriş Yap / Üye Ol</a>
                    </div>

                    <!-- Cart Button -->
                    <div class="cart-btn">
                        <p><span class="icon-shopping-cart"></span> <span class="quantity">1</span></p>
                    </div>
                </div>
            </div>
        </div>
    </nav>
    <!-- Navbar End -->
    <!-- Page Header Start -->
    <div class="container-fluid page-header py-6 wow fadeIn" data-wow-delay="0.1s">
        <div class="container text-center pt-5 pb-3">
            <h1 class="display-4 text-white animated slideInDown mb-3">@ViewData["Title"]</h1>

        </div>
    </div>
    <!-- Page Header End -->

    @RenderBody()


